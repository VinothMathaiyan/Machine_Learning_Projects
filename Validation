import pandas as pd
import numpy as np

# Sample DataFrame
data = {'ID': ['A12345678', 'B9876543', 'C1234567', 'D123456789', 'EABC1234']}
df = pd.DataFrame(data)

# Create a boolean array with potential missing values
valid_id_pattern = r'^[a-zA-Z]\d{8}$'
boolean_array = df['ID'].str.match(valid_id_pattern)

# Handle missing values
boolean_array = boolean_array.fillna(False)

# Create valid and invalid DataFrames
valid_df = df[boolean_array]
invalid_df = df[~boolean_array]

print(valid_df)
print(invalid_df)
