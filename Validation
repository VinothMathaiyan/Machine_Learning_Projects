import pandas as pd

def extract_domains(df, email_column):
  """Extracts domains from a DataFrame column.

  Args:
    df: The pandas DataFrame.
    email_column: The name of the email column.

  Returns:
    A list of domain names.
  """

  domains = df[email_column].str.split('@').str[1].tolist()
  return domains

# Sample DataFrame
data = {'Email': ['john.doe@example.com', 'invalid_email', 'jane.smith@company.net']}
df = pd.DataFrame(data)

email_column = 'Email'

domain_list = extract_domains(df, email_column)
print(domain_list)
