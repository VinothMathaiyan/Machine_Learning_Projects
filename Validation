import pandas as pd

def split_to_sheets(df, id_column, file_path):
  """Splits a DataFrame into different sheets of an Excel file based on duplicate IDs.

  Args:
    df: The pandas DataFrame.
    id_column: The name of the ID column.
    file_path: The path to save the Excel file.
  """

  duplicates_df = df[df[id_column].duplicated(keep=False)]
  unique_df = df[~df[id_column].duplicated()]

  with pd.ExcelWriter(file_path) as writer:
    duplicates_df.to_excel(writer, sheet_name='Duplicates', index=False)
    unique_df.to_excel(writer, sheet_name='Unique', index=False)

# Sample DataFrame
data = {'ID': ['G12345', 'G67890', 'G12345', 'G98765'],
        'DateOfBirth': ['24/09/1974', '15/12/1980', '24/09/1974', '01/01/2000'],
        'Access': ['G1234524091974', 'G6789015121980', 'G1234524091974', 'G9876501012000']}
df = pd.DataFrame(data)

id_column = 'ID'
file_path = 'output.xlsx'

split_to_sheets(df, id_column, file_path)
