import pandas as pd

def find_duplicate_ids(df, id_column):
  """Finds duplicate IDs in a DataFrame.

  Args:
    df: The pandas DataFrame.
    id_column: The name of the ID column.

  Returns:
    A list of duplicate IDs.
  """

  duplicate_ids = df[id_column].duplicated().index[df[id_column].duplicated()].tolist()
  return duplicate_ids

# Sample DataFrame
data = {'ID': ['G12345', 'G67890', 'G12345', 'G98765'],
        'DateOfBirth': ['24/09/1974', '15/12/1980', '24/09/1974', '01/01/2000'],
        'Access': ['G1234524091974', 'G6789015121980', 'G1234524091974', 'G9876501012000']}
df = pd.DataFrame(data)

id_column = 'ID'

duplicate_ids = find_duplicate_ids(df, id_column)
print("Duplicate IDs:", duplicate_ids)
