import pandas as pd

# Sample DataFrame with UserNames
data = {
    'UserName': ['X12345678#Name', 'G98765432#Name', 'X12345#Name', 'G123456789#Name', 'X87654321#Name', 'A12345678#Name']
}
df = pd.DataFrame(data)

# Function to validate UserName format
def is_valid_username(username):
    # Check if username ends with '#Name'
    if not username.endswith('#Name'):
        return False
    # Check if the part before '#Name' is a valid ID (first letter X or G followed by 8 digits)
    id_part = username[:-5]  # Remove '#Name' part
    return (len(id_part) == 9) and (id_part[0] in ['X', 'G']) and id_part[1:].isdigit()

# Validate and collect error UserNames
error_usernames = []
for username in df['UserName']:
    if not is_valid_username(username):
        error_usernames.append(username)

# Print the list of error UserNames
print("Error UserNames:", error_usernames)
