import pandas as pd

def find_duplicate_id_indices(df, id_column):
  """Finds indices of duplicate IDs in a DataFrame.

  Args:
    df: The pandas DataFrame.
    id_column: The name of the ID column.

  Returns:
    A list of indices corresponding to duplicate IDs.
  """

  duplicate_indices = df[df[id_column].duplicated(keep=False)].index.tolist()
  return duplicate_indices

# Sample DataFrame
data = {'ID': ['G12345', 'G67890', 'G12345', 'G98765'],
        'DateOfBirth': ['24/09/1974', '15/12/1980', '24/09/1974', '01/01/2000'],
        'Access': ['G1234524091974', 'G6789015121980', 'G1234524091974', 'G9876501012000']}
df = pd.DataFrame(data)

id_column = 'ID'

duplicate_indices = find_duplicate_id_indices(df, id_column)
print("Duplicate ID indices:", duplicate_indices)
